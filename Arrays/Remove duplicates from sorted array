int n = nums.length;
        int count = 0;
        // int[] temp = new int [n];
        if ( n==0 || n==1)
            return n;
        for (int i = 0; i<n-1; i++){
            if(nums[i] != nums[i+1]){
                nums[count]= nums[i];
                count++;
            }
        }
        nums[count++]= nums[n-1];
        
//         for(int i = 0; i<count; i++){
//             nums[i] = temp[i];
//         }
        
        return count;
        
